{
  "name": "@couture/db",
  "private": true,
  "version": "0.1.0",
  "description": "CoutureCast Prisma schema, migrations, and seeds",
  "type": "module",
  "scripts": {
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset --force --skip-generate --skip-seed && prisma db seed",
    "db:studio": "prisma studio",
    "db:generate": "prisma generate"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "@prisma/client": "6.19.0"
  },
  "devDependencies": {
    "prisma": "6.19.0",
    "tsx": "^4.20.6",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=22"
  },
  "prisma": {
    "seed": "tsx prisma/seeds/index.ts"
  }
}
